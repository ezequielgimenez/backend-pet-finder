{
  "version": 2,
  "builds": [
    {
      "src": "be-dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["be-dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "be-dist/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
